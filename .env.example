# AI API 配置
AI_PROVIDER=volcano  # 选择: openai 或 volcano (默认volcano)

# OpenAI API (备选)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o

# 火山引擎 API (豆包大模型 - 推荐)
VOLCANO_API_KEY=your_volcano_api_key_here
VOLCANO_API_SECRET=your_volcano_api_secret_here
VOLCANO_MODEL=doubao-pro-32k

# 图像生成 API
IMAGE_API_KEY=your_image_api_key_here
IMAGE_API_BASE=https://api.openai.com/v1
IMAGE_MODEL=dall-e-3

# 小红书配置
XIAOHONGSHU_COOKIE=your_xiaohongshu_cookie_here

# 公众号配置
WECHAT_APPID=your_wechat_appid_here
WECHAT_APPSECRET=your_wechat_appsecret_here
WECHAT_TOKEN=your_wechat_token_here
